<div class="container">

    <div class="row">

        <div class="col-md-4 offset-md-4">
            <div class="card text-center p-2">
                <img src="data:image/png;base64,"
                    class="card-img-top" style="max-width: 250px; max-height: 250px; margin: auto;">
                <div class="card-body text-center">
                    <h3 class="card-title">
                        {{user.name}}
                    </h3>
                    <h4 class="card-subtitle mb-2">
                        Your score is
                    </h4>
                    <h1>{{average}}</h1>
                </div>
            </div>
        </div>
    </div>
</div>