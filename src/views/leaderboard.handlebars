<h2>Leaderboard</h2>

<div class="table-responsive">

    <table class="table table-striped table-bordered table-sm">
        <thead class="thead-light">
            <tr>
                <th>Name</th>
                <th>Score</th>
            </tr>
        </thead>
        {{#each users}}
        <tr>
            <td>{{this.name}}</td>
            <td>{{this.score}}</td>
        </tr>
        {{/each}}
    </table>

</div>